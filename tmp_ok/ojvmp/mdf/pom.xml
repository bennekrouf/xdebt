        </profile>

        <profile>
            <id>dgnsi</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <profile.scm.connexion>scm:git:ssh://git@bitbucket.etat-de-vaud.ch/ojvmp/mdf.git</profile.scm.connexion>
                <profile.scm.developerConnection>scm:git:ssh://git@bitbucket.etat-de-vaud.ch/ojvmp/mdf.git
                </profile.scm.developerConnection>
                <profile.scm.url>scm:git:ssh://git@bitbucket.etat-de-vaud.ch/ojvmp/mdf.git</profile.scm.url>
                <profile.scm.tag>HEAD</profile.scm.tag>
            </properties>

            <distributionManagement>
                <repository>
                    <id>nexus-dgnsi-server</id>
                    <name>Nexus DGNSI</name>
                    <url>${nexus.url}/app-${application.repository}-stable</url>
                </repository>
                <snapshotRepository>
                    <id>nexus-dgnsi-server</id>
                    <name>Nexus DGNSI</name>
                    <url>${nexus.url}/app-${application.repository}-snapshots</url>
                    <uniqueVersion>false</uniqueVersion>
                </snapshotRepository>
            </distributionManagement>


        </profile>

        <profile>
            <id>not</id>
            <!-- NOTests -->
            <properties>
                <skipTests>true</skipTests>
            </properties>
        </profile>
    </profiles>

