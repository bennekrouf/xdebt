        <maven-release-plugin.version>3.0.1</maven-release-plugin.version>
        <maven-resources-plugin.version>3.2.0</maven-resources-plugin.version>
        <maven-dependency-plugin.version>3.6.1</maven-dependency-plugin.version>

        <normes-xsd.path>target/classes/xsd</normes-xsd.path>
        <enum-binding.path>src/main/resources/binding/enums</enum-binding.path>
        <enum-binding.file-name>enum-binding.xjb</enum-binding.file-name>
        <global-binding>${enum-binding.path}/global-binding.xjb</global-binding>
    </properties>

    <distributionManagement>
        <repository>
            <id>nexus-dgnsi-server</id>
            <name>Nexus DGNSI</name>
            <url>${nexus.url.stable}</url>
        </repository>
        <snapshotRepository>
            <id>nexus-dgnsi-server</id>
            <name>Nexus DGNSI</name>
            <url>${nexus.url.snapshots}</url>
            <uniqueVersion>false</uniqueVersion>
        </snapshotRepository>
    </distributionManagement>

    <dependencies>
        <dependency>
            <groupId>ch.vd.shared</groupId>
            <artifactId>normes-xsd</artifactId>
            <version>${normes-xsd.version}</version>
            <type>tar.gz</type>
        </dependency>

        <dependency>
            <groupId>jakarta.xml.bind</groupId>
            <artifactId>jakarta.xml.bind-api</artifactId>
            <version>${jakarta-xml.version}</version>
