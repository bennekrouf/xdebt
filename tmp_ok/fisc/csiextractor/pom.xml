<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>ch.vd.fiscalite</groupId>
    <artifactId>csiextractor</artifactId>
    <version>1.0.5-SNAPSHOT</version>
    <packaging>war</packaging>

    <name>CSI Extractor fiscalit√©</name>

    <parent>
        <groupId>ch.vd.cyber.parent</groupId>
        <artifactId>application-parent</artifactId>
        <version>12.12.0</version>
        <relativePath />
    </parent>

    <scm>
        <developerConnection>scm:git:ssh://bitbucket.etat-de-vaud.ch/fisc/csiextractor.git</developerConnection>
        <tag>HEAD</tag>
    </scm>

    <dependencies>
        <!-- standard deps -->
        <dependency>
            <groupId>ch.vd.cyber</groupId>
            <artifactId>starter-prestation</artifactId>
        </dependency>

        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <scope>provided</scope>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-actuator</artifactId>
        </dependency>

        <!-- specific deps -->
        <dependency>
            <groupId>ch.ewv</groupId>
            <artifactId>taxstatement</artifactId>
            <version>2.2.3</version>
        </dependency>

        <!-- Deps de test -->
        <dependency>
            <groupId>ch.vd.cybersdk</groupId>
            <artifactId>prestations-be</artifactId>
            <type>test-jar</type>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>org.springframework.security</groupId>
            <artifactId>spring-security-test</artifactId>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
            <exclusions>
                <exclusion>
                    <groupId>junit</groupId>
                    <artifactId>junit</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>org.junit.vintage</groupId>
                    <artifactId>junit-vintage-engine</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>org.junit.jupiter</groupId>
            <artifactId>junit-jupiter-api</artifactId>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>org.xmlunit</groupId>
            <artifactId>xmlunit-core</artifactId>
            <version>2.8.2</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>org.xmlunit</groupId>
            <artifactId>xmlunit-matchers</artifactId>
            <version>2.8.2</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>com.java4less</groupId>
            <artifactId>rbarcode</artifactId>
            <version>1.0</version>
        </dependency>

        <dependency>
            <groupId>com.java4less</groupId>
            <artifactId>rpdf417vision</artifactId>
            <version>1.0</version>
        </dependency>

        <dependency>
            <groupId>com.java4less</groupId>
            <artifactId>rvision</artifactId>
            <version>1.0</version>
        </dependency>

        <dependency>
            <groupId>com.java4less</groupId>
            <artifactId>r1dvision</artifactId>
            <version>1.0</version>
        </dependency>

        <dependency>
            <groupId>org.apache.pdfbox</groupId>
            <artifactId>pdfbox</artifactId> <!-- brings commons-logging, fontbox -->
            <version>2.0.22</version>
        </dependency>

        <dependency>
            <groupId>org.bouncycastle</groupId>
            <artifactId>bcpkix-jdk15on</artifactId> <!-- brings bcprov-jdk15on-->
            <version>1.58</version>
        </dependency>

        <dependency>
            <groupId>org.apache.santuario</groupId>
            <artifactId>xmlsec</artifactId>
            <version>2.0.8</version>
        </dependency>

        <dependency>
            <groupId>org.apache.poi</groupId>
            <artifactId>poi-ooxml</artifactId> <!-- brings poi + poi-ooxml + curvesapi -->
            <version>3.17</version>
        </dependency>

        <dependency>
            <groupId>org.apache.poi</groupId>
            <artifactId>poi-scratchpad</artifactId>
            <version>3.17</version>
        </dependency>

    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <configuration>
                    <forkCount>1</forkCount>
                    <reuseForks>false</reuseForks>
                </configuration>
            </plugin>

        </plugins>
    </build>

    <properties>
        <devex.configTag>master</devex.configTag>
    </properties>

</project>