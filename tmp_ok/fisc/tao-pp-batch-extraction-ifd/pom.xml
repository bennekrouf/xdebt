<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>ch.vd.dfin.tao</groupId>
        <artifactId>tao-parent-plugin</artifactId>
        <version>1.0.0</version>
    </parent>

    <groupId>ch.vd.dfin.tao.tao-pp</groupId>
    <artifactId>tao-pp-batch-extraction-ifd-parent</artifactId>
    <version>6.0.1-SNAPSHOT</version>
    <name>tao-pp-batch-extraction-ifd-parent</name>

    <packaging>pom</packaging>

    <modules>
        <module>batch</module>
        <module>distribution</module>
    </modules>

    <properties>
    <project.encoding>Cp1252</project.encoding>
    </properties>

    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <artifactId>maven-deploy-plugin</artifactId>
                    <configuration>
                        <skip>true</skip>
                    </configuration>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>

    <scm>
        <connection>scm:git:ssh://git@bitbucket.etat-de-vaud.ch/fisc/tao-pp-batch-extraction-ifd.git</connection>
        <developerConnection>scm:git:ssh://git@bitbucket.etat-de-vaud.ch/fisc/tao-pp-batch-extraction-ifd.git</developerConnection>
        <url>https://dsigit.etat-de-vaud.ch/outils/git/projects/FISC/repos/tao-pp-batch-extraction-ifd/browse</url>
        <tag>HEAD</tag>
    </scm>

    <distributionManagement>
        <repository>
            <id>nexus.releases</id>
            <name>Nexus releases</name>
            <url>${nexus.url}/app-${application.repository}-stable</url>
        </repository>
        <snapshotRepository>
            <id>nexus.snapshots</id>
            <name>Nexus snaphots</name>
            <url>${nexus.url}/app-${application.repository}-snapshots</url>
            <uniqueVersion>false</uniqueVersion>
        </snapshotRepository>
    </distributionManagement>
</project>