    <profiles>

        <!-- Le profil CLOUD permet de construire un WAR valable pour le cloud -->
        <!-- Ce war contient les statics et un fichier de contexte propre au cloud -->
        <profile>
            <id>cloud</id>
            <build>
                <plugins>
                    <plugin>
                        <!-- http://maven.apache.org/plugins/maven-clean-plugin/ -->
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-clean-plugin</artifactId>
                        <version>2.3</version>
                        <executions>
                            <execution>
                                <id>clean-before-assembly</id>
                                <phase>initialize</phase>
                                <goals>
                                    <goal>clean</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-resources-plugin</artifactId>
                        <version>2.3</version>
                        <executions>
                            <execution>
                                <id>copy-releases-notes</id>
                                <phase>process-resources</phase>
                                <goals>
                                    <goal>copy-resources</goal>
                                </goals>
                                <configuration>
                                    <outputDirectory>src/main/webapp/admin-deploy</outputDirectory>
                                    <resources>
                                        <resource>
                                            <directory>doc</directory>
                                            <includes>
                                                <include>release-notes.txt</include>
                                            </includes>
                                            <filtering>false</filtering>
                                        </resource>
                                    </resources>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                    <plugin>
                        <!-- http://maven.apache.org/plugins/maven-war-plugin/ -->
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-war-plugin</artifactId>
                        <version>2.1-beta-1</version>
                        <configuration>
                            <containerConfigXML>${basedir}/config/cloud/context.xml</containerConfigXML>
                            <webResources>
                                <resource>
                                    <directory>src/main/webapp/admin-deploy</directory>
                                    <includes>
                                        <include>index.jsp</include>
                                    </includes>
                                    <filtering>true</filtering>
                                    <targetPath>admin-deploy</targetPath>
                                </resource>
                            </webResources>
                            <archive>
                                <manifestEntries>
                                    <Application-Version>${project.version}</Application-Version>
                                </manifestEntries>
                            </archive>
                        </configuration>
                        <!-- <configuration> -->
                        <!-- <webXml>${basedir}/target/jspweb.xml</webXml> -->
                        <!-- </configuration> -->
                    </plugin>
                </plugins>
            </build>
        </profile>
