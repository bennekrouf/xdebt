<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>ch.vd.ses</groupId>
	<artifactId>medreg-parent</artifactId>
	<version>6.2.8-SNAPSHOT</version>
	<packaging>pom</packaging>
	<repositories>
		<repository>
			<id>releases</id>
			<name>Releases de l'Etat de Vaud</name>
			<url>${nexus.url.releases}</url>
		</repository>
		<repository>
			<id>snapshots</id>
			<name>Snapshots de l'Etat de Vaud</name>
			<url>${nexus.url.snapshots}</url>
		</repository>
	</repositories>
	<scm>
		<url>https://portail.etat-de-vaud.ch/outils/git/projects/ses/repos/medreg</url>
		<developerConnection>scm:git:ssh://git@bitbucket.etat-de-vaud.ch/ses/medreg.git</developerConnection>
		<connection>scm:git:ssh://git@bitbucket.etat-de-vaud.ch/ses/medreg.git</connection>
	  <tag>6.2.2</tag>
  </scm>
	<issueManagement>
		<system>JIRA</system>
		<url>http://issuetracker.etat-de-vaud.ch/outils/jira/secure/Dashboard.jspa</url>
	</issueManagement>
	<properties>
		<spring.version>4.3.4.RELEASE</spring.version>
    <product.name>medreg</product.name>
		<!-- <medreg.name>medreg</medreg.name> -->
		<java.version>1.8</java.version>
		<shared-iam.version>3.5.0</shared-iam.version>
		<project.build.sourceEncoding>ISO-8859-1</project.build.sourceEncoding>
		<assembly.finalName>medreg-${project.version}</assembly.finalName>
		<maven-assembly-plugin.version>3.1.1</maven-assembly-plugin.version>
    <maven-release-plugin.version>2.5.3</maven-release-plugin.version>
		<log4j2.version>2.17.2</log4j2.version>

		<release.profiles>not,assembly,nexus,${devex.profiles}</release.profiles>
		<!-- DevEx -->
		<devex-maven-plugin.version>1.0.19</devex-maven-plugin.version>
		<devex.profiles>devex.release,devex.in</devex.profiles>
		<!-- TODO a changer apres la fin de la migration  -->
		<devex-maven-plugin.configTag>feature_TMAPASS-666</devex-maven-plugin.configTag>
		<devex.releaseVersion>${project.version}</devex.releaseVersion>
		<devex.deployUnit>medreg</devex.deployUnit>
		<application.repository>jenkinsses</application.repository>

		<!-- maven release plugin configuration for applications -->
		<release.goals>clean install</release.goals>
		<release.pushChanges>true</release.pushChanges>
		<error-prone.version>2.10.0</error-prone.version>
		<javac.version>9+181-r4173-1</javac.version>
		<sonar.java.coveragePlugin>jacoco</sonar.java.coveragePlugin>
		<sonar.coverage.jacoco.xmlReportPaths>${project.basedir}/../medreg-web/target/site/jacoco-aggregate/jacoco.xml</sonar.coverage.jacoco.xmlReportPaths>

	</properties>
	<modules>
		<module>medreg-rest</module>
		<module>medreg-core</module>
		<module>medreg-web</module>
		<module>medreg-dist</module>
	</modules>
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>junit</groupId>
				<artifactId>junit</artifactId>
				<version>4.13.2</version>
				<scope>test</scope>
			</dependency>
			<dependency>
				<groupId>org.springframework</groupId>
				<artifactId>spring-test</artifactId>
				<version>${spring.version}</version>
				<scope>test</scope>
			</dependency>
		</dependencies>
	</dependencyManagement>
	<build>
		<pluginManagement>
			<plugins>
				<!-- default version 2.10 has transfer problems from nexus (spip) -->
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-surefire-plugin</artifactId>
					<version>2.19.1</version>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>3.8.1</version>
					<configuration>
						<source>${java.version}</source>
						<target>${java.version}</target>
						<compilerArgs>
							<arg>-XDcompilePolicy=simple</arg>
							<arg>-Xplugin:ErrorProne -XepDisableWarningsInGeneratedCode</arg>
						</compilerArgs>
						<annotationProcessorPaths>
							<path>
								<groupId>com.google.errorprone</groupId>
								<artifactId>error_prone_core</artifactId>
								<version>${error-prone.version}</version>
							</path>
						</annotationProcessorPaths>
					</configuration>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-resources-plugin</artifactId>
					<version>2.7</version>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-assembly-plugin</artifactId>
          <version>${maven-assembly-plugin.version}</version>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-release-plugin</artifactId>
          <version>${maven-release-plugin.version}</version>
					<configuration>
						<goals>${release.goals}</goals>
						<releaseProfiles>${release.profiles}</releaseProfiles>
						<tagNameFormat>@{project.version}</tagNameFormat>
						<arguments>-Dmaven.javadoc.skip=true</arguments>
						<pushChanges>${release.pushChanges}</pushChanges>
					</configuration>
				</plugin>
				<plugin>
					<groupId>ch.vd.devex</groupId>
					<artifactId>devex-maven-plugin</artifactId>
					<version>${devex-maven-plugin.version}</version>
					<configuration>
						<configTag>${devex-maven-plugin.configTag}</configTag>
						<releaseVersion>${devex.releaseVersion}</releaseVersion>
						<deployUnit>${devex.deployUnit}</deployUnit>
					</configuration>
				</plugin>
				<plugin>
					<groupId>org.jacoco</groupId>
					<artifactId>jacoco-maven-plugin</artifactId>
					<version>0.8.8</version>
					<executions>
						<execution>
							<id>prepare-agent</id>
							<goals>
								<goal>prepare-agent</goal>
							</goals>
						</execution>
					</executions>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
	<profiles>
	  <profile>
			<id>nexus</id>
			<properties>
				<release.goals>clean deploy</release.goals>
				<devex-maven-plugin.phase>deploy</devex-maven-plugin.phase>
			</properties>
		</profile>
		<profile>
			<id>jdk8</id>
			<activation>
				<jdk>1.8</jdk>
			</activation>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-compiler-plugin</artifactId>
						<configuration>
							<fork>true</fork>
							<compilerArgs combine.children="append">
								<arg>
									-J-Xbootclasspath/p:${settings.localRepository}/com/google/errorprone/javac/${javac.version}/javac-${javac.version}.jar
								</arg>
							</compilerArgs>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
	</profiles>
	<distributionManagement>
		<repository>
			<id>nexus-dgnsi-server</id>
			<name>Nexus DGNSI</name>
			<url>${nexus.url}/app-${application.repository}-stable</url>
		</repository>
		<snapshotRepository>
			<id>nexus-dgnsi-server</id>
			<name>Nexus DGNSI</name>
			<url>${nexus.url}/app-${application.repository}-snapshots</url>
			<uniqueVersion>false</uniqueVersion>
		</snapshotRepository>
	</distributionManagement>
</project>
